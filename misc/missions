static void	ft_near_star_UL(t_data *data, int y, int x)
{
	char *star;
	int i;

	i = x;
	if (data->piece[y][x] != '*')
	{
		while (y >= 0 && star != NULL)
		{
			star = ft_strchr(data->piece[y--], '*');
		}
		y++;
		if (star != NULL)
		{
			while (x >= 0 && data->piece[y][x] != '*')
			{
				x--;
			}
		}
	}
		data->coo[0] = data->coo_me[0] - y;
		data->coo[1] = data->coo_me[1] - x;

}
